class Person
{
    init(name, age)
    {
        this.name = name;
        this.age = age;
    }

    info()
    {
        print "Name: " + this.name;
        print "Age: " + this.age;
    }

    greeting()
    {
        print "Hello, my name is " + this.name + " and I am " + this.age + " years old.";
    }
}

var person = Person("Hannah", 31);
person.height = 54;

person.info();
print person.height;
person.greeting();
